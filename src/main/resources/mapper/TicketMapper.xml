<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.nbcj.travelshare.mapper.TicketMapper">
<!--    <select id=""></select>-->

    <!-- 获取景区内所有景点门票 -->
    <select id="getTicketBySid" resultType="com.nbcj.travelshare.domain.Ticket">
        SELECT *
        FROM ticket
        WHERE sid = #{sid}
    </select>
</mapper>