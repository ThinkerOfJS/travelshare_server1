<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.nbcj.travelshare.mapper.AttractionsMapper">
<!--    <select id=""></select>-->

    <select id="getAttractionsBySid" resultType="com.nbcj.travelshare.domain.Attractions">
        SELECT *
        FROM attractions
        WHERE sid = #{sid}
    </select>
</mapper>